--[[
    VoidMobile UI Library - Compact Edition
    Optimized for mobile screens (square-ish design)
    Purple-black theme with smooth animations
]]--

local VoidMobile = {}

-- Services
local TweenService = game:GetService("TweenService")
local UserInputService = game:GetService("UserInputService")
local Players = game:GetService("Players")
local CoreGui = game:GetService("CoreGui")

-- Utility Functions
local function Tween(obj, props, time)
    TweenService:Create(obj, TweenInfo.new(time or 0.25, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), props):Play()
end

local function Ripple(btn, x, y)
    spawn(function()
        btn.ClipsDescendants = true
        local c = Instance.new("ImageLabel")
        c.Name = "Ripple"
        c.BackgroundTransparency = 1
        c.Image = "rbxassetid://266543268"
        c.ImageTransparency = 0.5
        c.ZIndex = 1000
        c.Parent = btn
        
        local nx = x - c.AbsolutePosition.X
        local ny = y - c.AbsolutePosition.Y
        c.Position = UDim2.new(0, nx, 0, ny)
        
        local size = math.max(btn.AbsoluteSize.X, btn.AbsoluteSize.Y) * 1.5
        c:TweenSizeAndPosition(
            UDim2.new(0, size, 0, size),
            UDim2.new(0.5, -size/2, 0.5, -size/2),
            "Out", "Quad", 0.4, false
        )
        Tween(c, {ImageTransparency = 1}, 0.4)
        task.wait(0.4)
        c:Destroy()
    end)
end

-- Cleanup
for _, v in pairs(CoreGui:GetChildren()) do
    if v.Name == "VoidMobileUI" then
        v:Destroy()
    end
end

function VoidMobile:CreateWindow(config)
    config = config or {}
    local name = config.Name or "VoidMobile"
    local color = config.Color or Color3.fromRGB(120, 60, 180)
    
    local ScreenGui = Instance.new("ScreenGui")
    ScreenGui.Name = "VoidMobileUI"
    ScreenGui.ResetOnSpawn = false
    ScreenGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
    ScreenGui.Parent = CoreGui
    
    -- Main Frame (Square-ish design)
    local Main = Instance.new("Frame")
    Main.Name = "Main"
    Main.Size = UDim2.fromOffset(380, 420)
    Main.Position = UDim2.new(0.5, -190, 0.5, -210)
    Main.BackgroundColor3 = Color3.fromRGB(15, 10, 20)
    Main.BorderSizePixel = 0
    Main.Active = true
    Main.Parent = ScreenGui
    
    local MainCorner = Instance.new("UICorner")
    MainCorner.CornerRadius = UDim.new(0, 12)
    MainCorner.Parent = Main
    
    local MainStroke = Instance.new("UIStroke")
    MainStroke.Color = color
    MainStroke.Thickness = 2
    MainStroke.Parent = Main
    
    -- Shadow
    local Shadow = Instance.new("ImageLabel")
    Shadow.Name = "Shadow"
    Shadow.Image = "rbxassetid://297774371"
    Shadow.ImageColor3 = Color3.fromRGB(80, 40, 120)
    Shadow.ImageTransparency = 0.4
    Shadow.ScaleType = Enum.ScaleType.Slice
    Shadow.SliceCenter = Rect.new(20, 20, 280, 280)
    Shadow.BackgroundTransparency = 1
    Shadow.Size = UDim2.new(1, 30, 1, 30)
    Shadow.Position = UDim2.fromOffset(-15, -15)
    Shadow.ZIndex = -1
    Shadow.Parent = Main
    
    -- Title Bar
    local TitleBar = Instance.new("Frame")
    TitleBar.Name = "TitleBar"
    TitleBar.Size = UDim2.new(1, 0, 0, 40)
    TitleBar.BackgroundColor3 = Color3.fromRGB(25, 15, 35)
    TitleBar.BorderSizePixel = 0
    TitleBar.Parent = Main
    
    local TitleCorner = Instance.new("UICorner")
    TitleCorner.CornerRadius = UDim.new(0, 12)
    TitleCorner.Parent = TitleBar
    
    local TitleGrad = Instance.new("UIGradient")
    TitleGrad.Color = ColorSequence.new{
        ColorSequenceKeypoint.new(0, color),
        ColorSequenceKeypoint.new(1, Color3.fromRGB(25, 15, 35))
    }
    TitleGrad.Rotation = 90
    TitleGrad.Parent = TitleBar
    
    local Title = Instance.new("TextLabel")
    Title.Name = "Title"
    Title.Size = UDim2.new(1, -90, 1, 0)
    Title.Position = UDim2.fromOffset(12, 0)
    Title.BackgroundTransparency = 1
    Title.Text = name
    Title.Font = Enum.Font.GothamBold
    Title.TextSize = 18
    Title.TextColor3 = Color3.white
    Title.TextXAlignment = Enum.TextXAlignment.Left
    Title.Parent = TitleBar
    
    -- Close Button
    local Close = Instance.new("TextButton")
    Close.Name = "Close"
    Close.Size = UDim2.fromOffset(32, 32)
    Close.Position = UDim2.new(1, -40, 0, 4)
    Close.BackgroundColor3 = Color3.fromRGB(180, 50, 80)
    Close.Text = "✕"
    Close.Font = Enum.Font.GothamBold
    Close.TextSize = 18
    Close.TextColor3 = Color3.white
    Close.AutoButtonColor = false
    Close.Parent = TitleBar
    Instance.new("UICorner", Close).CornerRadius = UDim.new(0, 8)
    
    Close.MouseButton1Click:Connect(function()
        Tween(Main, {Size = UDim2.fromOffset(0, 0)}, 0.3)
        task.wait(0.3)
        ScreenGui:Destroy()
    end)
    
    Close.MouseEnter:Connect(function()
        Tween(Close, {BackgroundColor3 = Color3.fromRGB(220, 60, 100)}, 0.2)
    end)
    
    Close.MouseLeave:Connect(function()
        Tween(Close, {BackgroundColor3 = Color3.fromRGB(180, 50, 80)}, 0.2)
    end)
    
    -- Minimize Button
    local Minimize = Instance.new("TextButton")
    Minimize.Name = "Minimize"
    Minimize.Size = UDim2.fromOffset(32, 32)
    Minimize.Position = UDim2.new(1, -76, 0, 4)
    Minimize.BackgroundColor3 = color
    Minimize.Text = "−"
    Minimize.Font = Enum.Font.GothamBold
    Minimize.TextSize = 20
    Minimize.TextColor3 = Color3.white
    Minimize.AutoButtonColor = false
    Minimize.Parent = TitleBar
    Instance.new("UICorner", Minimize).CornerRadius = UDim.new(0, 8)
    
    Minimize.MouseEnter:Connect(function()
        Tween(Minimize, {BackgroundColor3 = Color3.fromRGB(140, 70, 200)}, 0.2)
    end)
    
    Minimize.MouseLeave:Connect(function()
        Tween(Minimize, {BackgroundColor3 = color}, 0.2)
    end)
    
    -- Floating Restore Button (Hidden by default)
    local RestoreBtn = Instance.new("TextButton")
    RestoreBtn.Name = "RestoreBtn"
    RestoreBtn.Size = UDim2.fromOffset(50, 50)
    RestoreBtn.Position = UDim2.new(1, -60, 0, 10)
    RestoreBtn.BackgroundColor3 = color
    RestoreBtn.Text = "+"
    RestoreBtn.Font = Enum.Font.GothamBold
    RestoreBtn.TextSize = 24
    RestoreBtn.TextColor3 = Color3.white
    RestoreBtn.AutoButtonColor = false
    RestoreBtn.Visible = false
    RestoreBtn.Parent = ScreenGui
    RestoreBtn.ZIndex = 1000
    
    local RestoreCorner = Instance.new("UICorner")
    RestoreCorner.CornerRadius = UDim.new(0, 25)
    RestoreCorner.Parent = RestoreBtn
    
    local RestoreStroke = Instance.new("UIStroke")
    RestoreStroke.Color = Color3.white
    RestoreStroke.Thickness = 2
    RestoreStroke.Parent = RestoreBtn
    
    local RestoreShadow = Instance.new("ImageLabel")
    RestoreShadow.Image = "rbxassetid://297774371"
    RestoreShadow.ImageColor3 = Color3.fromRGB(80, 40, 120)
    RestoreShadow.ImageTransparency = 0.3
    RestoreShadow.ScaleType = Enum.ScaleType.Slice
    RestoreShadow.SliceCenter = Rect.new(20, 20, 280, 280)
    RestoreShadow.BackgroundTransparency = 1
    RestoreShadow.Size = UDim2.new(1, 20, 1, 20)
    RestoreShadow.Position = UDim2.fromOffset(-10, -10)
    RestoreShadow.ZIndex = -1
    RestoreShadow.Parent = RestoreBtn
    
    -- Drag restore button
    local restoreDragging, restoreDragInput, restoreDragStart, restoreStartPos
    
    RestoreBtn.InputBegan:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseButton1 or 
           input.UserInputType == Enum.UserInputType.Touch then
            restoreDragging = true
            restoreDragStart = input.Position
            restoreStartPos = RestoreBtn.Position
            
            input.Changed:Connect(function()
                if input.UserInputState == Enum.UserInputState.End then
                    restoreDragging = false
                end
            end)
        end
    end)
    
    RestoreBtn.InputChanged:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseMovement or
           input.UserInputType == Enum.UserInputType.Touch then
            restoreDragInput = input
        end
    end)
    
    UserInputService.InputChanged:Connect(function(input)
        if input == restoreDragInput and restoreDragging then
            local delta = input.Position - restoreDragStart
            Tween(RestoreBtn, {
                Position = UDim2.new(
                    restoreStartPos.X.Scale, restoreStartPos.X.Offset + delta.X,
                    restoreStartPos.Y.Scale, restoreStartPos.Y.Offset + delta.Y
                )
            }, 0.08)
        end
    end)
    
    RestoreBtn.MouseEnter:Connect(function()
        Tween(RestoreBtn, {Size = UDim2.fromOffset(55, 55)}, 0.2)
        Tween(RestoreBtn, {BackgroundColor3 = Color3.fromRGB(140, 70, 200)}, 0.2)
    end)
    
    RestoreBtn.MouseLeave:Connect(function()
        Tween(RestoreBtn, {Size = UDim2.fromOffset(50, 50)}, 0.2)
        Tween(RestoreBtn, {BackgroundColor3 = color}, 0.2)
    end)
    
    local minimized = false
    
    Minimize.MouseButton1Click:Connect(function()
        minimized = true
        Tween(Main, {Size = UDim2.fromOffset(0, 0)}, 0.3)
        Tween(Shadow, {ImageTransparency = 1}, 0.3)
        task.wait(0.3)
        Main.Visible = false
        RestoreBtn.Visible = true
        Tween(RestoreBtn, {Size = UDim2.fromOffset(50, 50)}, 0.3)
    end)
    
    RestoreBtn.MouseButton1Click:Connect(function()
        if not restoreDragging then
            minimized = false
            RestoreBtn.Visible = false
            Main.Visible = true
            Shadow.ImageTransparency = 0.4
            Tween(Main, {Size = UDim2.fromOffset(380, 420)}, 0.3)
        end
    end)
    
    -- Drag Main Window
    local dragging, dragInput, dragStart, startPos
    
    TitleBar.InputBegan:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseButton1 or 
           input.UserInputType == Enum.UserInputType.Touch then
            dragging = true
            dragStart = input.Position
            startPos = Main.Position
            
            input.Changed:Connect(function()
                if input.UserInputState == Enum.UserInputState.End then
                    dragging = false
                end
            end)
        end
    end)
    
    TitleBar.InputChanged:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseMovement or
           input.UserInputType == Enum.UserInputType.Touch then
            dragInput = input
        end
    end)
    
    UserInputService.InputChanged:Connect(function(input)
        if input == dragInput and dragging then
            local delta = input.Position - dragStart
            Tween(Main, {
                Position = UDim2.new(
                    startPos.X.Scale, startPos.X.Offset + delta.X,
                    startPos.Y.Scale, startPos.Y.Offset + delta.Y
                )
            }, 0.08)
        end
    end)
    
    -- Tab Bar
    local TabBar = Instance.new("ScrollingFrame")
    TabBar.Name = "TabBar"
    TabBar.Size = UDim2.new(1, -16, 0, 38)
    TabBar.Position = UDim2.fromOffset(8, 48)
    TabBar.BackgroundColor3 = Color3.fromRGB(20, 12, 28)
    TabBar.BorderSizePixel = 0
    TabBar.ScrollBarThickness = 0
    TabBar.CanvasSize = UDim2.fromOffset(0, 0)
    TabBar.AutomaticCanvasSize = Enum.AutomaticSize.X
    TabBar.ScrollingDirection = Enum.ScrollingDirection.X
    TabBar.Parent = Main
    Instance.new("UICorner", TabBar).CornerRadius = UDim.new(0, 8)
    
    local TabLayout = Instance.new("UIListLayout")
    TabLayout.FillDirection = Enum.FillDirection.Horizontal
    TabLayout.Padding = UDim.new(0, 6)
    TabLayout.Parent = TabBar
    
    local TabPadding = Instance.new("UIPadding")
    TabPadding.PaddingLeft = UDim.new(0, 6)
    TabPadding.PaddingTop = UDim.new(0, 5)
    TabPadding.PaddingRight = UDim.new(0, 6)
    TabPadding.Parent = TabBar
    
    -- Content Area
    local Content = Instance.new("Frame")
    Content.Name = "Content"
    Content.Size = UDim2.new(1, -16, 1, -102)
    Content.Position = UDim2.fromOffset(8, 94)
    Content.BackgroundColor3 = Color3.fromRGB(20, 12, 28)
    Content.BorderSizePixel = 0
    Content.Parent = Main
    Instance.new("UICorner", Content).CornerRadius = UDim.new(0, 8)
    
    -- Notification Container
    local NotifHolder = Instance.new("Frame")
    NotifHolder.Name = "NotifHolder"
    NotifHolder.Size = UDim2.new(0, 300, 1, 0)
    NotifHolder.Position = UDim2.new(1, -310, 0, 10)
    NotifHolder.BackgroundTransparency = 1
    NotifHolder.Parent = ScreenGui
    
    local NotifLayout = Instance.new("UIListLayout")
    NotifLayout.Padding = UDim.new(0, 8)
    NotifLayout.VerticalAlignment = Enum.VerticalAlignment.Top
    NotifLayout.Parent = NotifHolder
    
    local Window = {}
    Window.Tabs = {}
    Window.Color = color
    
    function Window:AddTab(tabName)
        local Tab = {}
        
        -- Tab Button
        local TabBtn = Instance.new("TextButton")
        TabBtn.Name = tabName
        TabBtn.Size = UDim2.fromOffset(90, 28)
        TabBtn.BackgroundColor3 = Color3.fromRGB(35, 20, 50)
        TabBtn.Text = tabName
        TabBtn.Font = Enum.Font.GothamBold
        TabBtn.TextSize = 13
        TabBtn.TextColor3 = Color3.fromRGB(160, 120, 200)
        TabBtn.AutoButtonColor = false
        TabBtn.Parent = TabBar
        Instance.new("UICorner", TabBtn).CornerRadius = UDim.new(0, 6)
        
        -- Tab Content
        local TabContent = Instance.new("ScrollingFrame")
        TabContent.Name = tabName.."Content"
        TabContent.Size = UDim2.new(1, -12, 1, -12)
        TabContent.Position = UDim2.fromOffset(6, 6)
        TabContent.BackgroundTransparency = 1
        TabContent.BorderSizePixel = 0
        TabContent.ScrollBarThickness = 4
        TabContent.ScrollBarImageColor3 = color
        TabContent.CanvasSize = UDim2.fromOffset(0, 0)
        TabContent.AutomaticCanvasSize = Enum.AutomaticSize.Y
        TabContent.Visible = false
        TabContent.Parent = Content
        
        local ContentLayout = Instance.new("UIListLayout")
        ContentLayout.Padding = UDim.new(0, 6)
        ContentLayout.Parent = TabContent
        
        TabBtn.MouseButton1Click:Connect(function()
            Ripple(TabBtn, game.Players.LocalPlayer:GetMouse().X, game.Players.LocalPlayer:GetMouse().Y)
            for _, tab in pairs(Window.Tabs) do
                tab.Button.BackgroundColor3 = Color3.fromRGB(35, 20, 50)
                tab.Button.TextColor3 = Color3.fromRGB(160, 120, 200)
                tab.Content.Visible = false
            end
            TabBtn.BackgroundColor3 = color
            TabBtn.TextColor3 = Color3.white
            TabContent.Visible = true
        end)
        
        TabBtn.MouseEnter:Connect(function()
            if TabBtn.BackgroundColor3 ~= color then
                Tween(TabBtn, {BackgroundColor3 = Color3.fromRGB(45, 25, 60)}, 0.2)
            end
        end)
        
        TabBtn.MouseLeave:Connect(function()
            if TabBtn.BackgroundColor3 ~= color then
                Tween(TabBtn, {BackgroundColor3 = Color3.fromRGB(35, 20, 50)}, 0.2)
            end
        end)
        
        Tab.Button = TabBtn
        Tab.Content = TabContent
        table.insert(Window.Tabs, Tab)
        
        if #Window.Tabs == 1 then
            TabBtn.BackgroundColor3 = color
            TabBtn.TextColor3 = Color3.white
            TabContent.Visible = true
        end
        
        -- Elements
        
        function Tab:Label(text)
            local LabelFrame = Instance.new("Frame")
            LabelFrame.Size = UDim2.new(1, -6, 0, 32)
            LabelFrame.BackgroundColor3 = Color3.fromRGB(30, 18, 40)
            LabelFrame.BorderSizePixel = 0
            LabelFrame.Parent = TabContent
            Instance.new("UICorner", LabelFrame).CornerRadius = UDim.new(0, 6)
            
            local Label = Instance.new("TextLabel")
            Label.Size = UDim2.new(1, -12, 1, 0)
            Label.Position = UDim2.fromOffset(8, 0)
            Label.BackgroundTransparency = 1
            Label.Text = text
            Label.Font = Enum.Font.Gotham
            Label.TextSize = 14
            Label.TextColor3 = Color3.fromRGB(200, 150, 255)
            Label.TextXAlignment = Enum.TextXAlignment.Left
            Label.TextWrapped = true
            Label.Parent = LabelFrame
            
            local LabelObj = {}
            function LabelObj:Set(newText)
                Label.Text = newText
            end
            return LabelObj
        end
        
        function Tab:Button(text, callback)
            callback = callback or function() end
            
            local BtnFrame = Instance.new("TextButton")
            BtnFrame.Size = UDim2.new(1, -6, 0, 38)
            BtnFrame.BackgroundColor3 = Color3.fromRGB(80, 40, 120)
            BtnFrame.Text = ""
            BtnFrame.AutoButtonColor = false
            BtnFrame.Parent = TabContent
            Instance.new("UICorner", BtnFrame).CornerRadius = UDim.new(0, 6)
            
            local BtnGrad = Instance.new("UIGradient")
            BtnGrad.Color = ColorSequence.new{
                ColorSequenceKeypoint.new(0, Color3.fromRGB(100, 50, 150)),
                ColorSequenceKeypoint.new(1, Color3.fromRGB(80, 40, 120))
            }
            BtnGrad.Rotation = 90
            BtnGrad.Parent = BtnFrame
            
            local BtnText = Instance.new("TextLabel")
            BtnText.Size = UDim2.new(1, -40, 1, 0)
            BtnText.Position = UDim2.fromOffset(10, 0)
            BtnText.BackgroundTransparency = 1
            BtnText.Text = text
            BtnText.Font = Enum.Font.GothamBold
            BtnText.TextSize = 15
            BtnText.TextColor3 = Color3.white
            BtnText.TextXAlignment = Enum.TextXAlignment.Left
            BtnText.Parent = BtnFrame
            
            local Icon = Instance.new("ImageLabel")
            Icon.Size = UDim2.fromOffset(18, 18)
            Icon.Position = UDim2.new(1, -26, 0.5, -9)
            Icon.BackgroundTransparency = 1
            Icon.Image = "rbxassetid://9584292194"
            Icon.ImageColor3 = Color3.white
            Icon.Parent = BtnFrame
            
            BtnFrame.MouseButton1Click:Connect(function()
                Ripple(BtnFrame, game.Players.LocalPlayer:GetMouse().X, game.Players.LocalPlayer:GetMouse().Y)
                Tween(BtnFrame, {BackgroundColor3 = Color3.fromRGB(120, 60, 180)}, 0.1)
                task.wait(0.1)
                Tween(BtnFrame, {BackgroundColor3 = Color3.fromRGB(80, 40, 120)}, 0.1)
                callback()
            end)
            
            BtnFrame.MouseEnter:Connect(function()
                Tween(BtnFrame, {BackgroundColor3 = Color3.fromRGB(100, 50, 150)}, 0.2)
            end)
            
            BtnFrame.MouseLeave:Connect(function()
                Tween(BtnFrame, {BackgroundColor3 = Color3.fromRGB(80, 40, 120)}, 0.2)
            end)
        end
        
        function Tab:Toggle(text, default, callback)
            callback = callback or function() end
            local toggled = default or false
            
            local TogFrame = Instance.new("TextButton")
            TogFrame.Size = UDim2.new(1, -6, 0, 38)
            TogFrame.BackgroundColor3 = Color3.fromRGB(30, 18, 40)
            TogFrame.Text = ""
            TogFrame.AutoButtonColor = false
            TogFrame.Parent = TabContent
            Instance.new("UICorner", TogFrame).CornerRadius = UDim.new(0, 6)
            
            local TogText = Instance.new("TextLabel")
            TogText.Size = UDim2.new(1, -60, 1, 0)
            TogText.Position = UDim2.fromOffset(10, 0)
            TogText.BackgroundTransparency = 1
            TogText.Text = text
            TogText.Font = Enum.Font.Gotham
            TogText.TextSize = 14
            TogText.TextColor3 = Color3.fromRGB(200, 150, 255)
            TogText.TextXAlignment = Enum.TextXAlignment.Left
            TogText.Parent = TogFrame
            
            local TogSwitch = Instance.new("Frame")
            TogSwitch.Size = UDim2.fromOffset(46, 24)
            TogSwitch.Position = UDim2.new(1, -54, 0.5, -12)
            TogSwitch.BackgroundColor3 = toggled and color or Color3.fromRGB(40, 25, 50)
            TogSwitch.BorderSizePixel = 0
            TogSwitch.Parent = TogFrame
            Instance.new("UICorner", TogSwitch).CornerRadius = UDim.new(0, 12)
            
            local TogCircle = Instance.new("Frame")
            TogCircle.Size = UDim2.fromOffset(20, 20)
            TogCircle.Position = toggled and UDim2.fromOffset(24, 2) or UDim2.fromOffset(2, 2)
            TogCircle.BackgroundColor3 = Color3.white
            TogCircle.BorderSizePixel = 0
            TogCircle.Parent = TogSwitch
            Instance.new("UICorner", TogCircle).CornerRadius = UDim.new(0, 10)
            
            TogFrame.MouseButton1Click:Connect(function()
                toggled = not toggled
                Ripple(TogFrame, game.Players.LocalPlayer:GetMouse().X, game.Players.LocalPlayer:GetMouse().Y)
                
                if toggled then
                    Tween(TogSwitch, {BackgroundColor3 = color}, 0.2)
                    Tween(TogCircle, {Position = UDim2.fromOffset(24, 2)}, 0.2)
                else
                    Tween(TogSwitch, {BackgroundColor3 = Color3.fromRGB(40, 25, 50)}, 0.2)
                    Tween(TogCircle, {Position = UDim2.fromOffset(2, 2)}, 0.2)
                end
                
                callback(toggled)
            end)
            
            TogFrame.MouseEnter:Connect(function()
                Tween(TogFrame, {BackgroundColor3 = Color3.fromRGB(35, 20, 45)}, 0.2)
            end)
            
            TogFrame.MouseLeave:Connect(function()
                Tween(TogFrame, {BackgroundColor3 = Color3.fromRGB(30, 18, 40)}, 0.2)
            end)
            
            local TogObj = {}
            function TogObj:Set(state)
                toggled = state
                if toggled then
                    Tween(TogSwitch, {BackgroundColor3 = color}, 0.2)
                    Tween(TogCircle, {Position = UDim2.fromOffset(24, 2)}, 0.2)
                else
                    Tween(TogSwitch, {BackgroundColor3 = Color3.fromRGB(40, 25, 50)}, 0.2)
                    Tween(TogCircle, {Position = UDim2.fromOffset(2, 2)}, 0.2)
                end
            end
            return TogObj
        end
        
        function Tab:Slider(text, min, max, default, callback)
            callback = callback or function() end
            local value = default or min
            
            local SliderFrame = Instance.new("Frame")
            SliderFrame.Size = UDim2.new(1, -6, 0, 50)
            SliderFrame.BackgroundColor3 = Color3.fromRGB(30, 18, 40)
            SliderFrame.BorderSizePixel = 0
            SliderFrame.Parent = TabContent
            Instance.new("UICorner", SliderFrame).CornerRadius = UDim.new(0, 6)
            
            local SliderText = Instance.new("TextLabel")
            SliderText.Size = UDim2.new(1, -60, 0, 18)
            SliderText.Position = UDim2.fromOffset(10, 6)
            SliderText.BackgroundTransparency = 1
            SliderText.Text = text
            SliderText.Font = Enum.Font.Gotham
            SliderText.TextSize = 14
            SliderText.TextColor3 = Color3.fromRGB(200, 150, 255)
            SliderText.TextXAlignment = Enum.TextXAlignment.Left
            SliderText.Parent = SliderFrame
            
            local ValueLabel = Instance.new("TextLabel")
            ValueLabel.Size = UDim2.fromOffset(45, 18)
            ValueLabel.Position = UDim2.new(1, -52, 0, 6)
            ValueLabel.BackgroundTransparency = 1
            ValueLabel.Text = tostring(value)
            ValueLabel.Font = Enum.Font.GothamBold
            ValueLabel.TextSize = 14
            ValueLabel.TextColor3 = Color3.fromRGB(150, 100, 200)
            ValueLabel.TextXAlignment = Enum.TextXAlignment.Right
            ValueLabel.Parent = SliderFrame
            
            local SliderBack = Instance.new("Frame")
            SliderBack.Size = UDim2.new(1, -20, 0, 6)
            SliderBack.Position = UDim2.fromOffset(10, 34)
            SliderBack.BackgroundColor3 = Color3.fromRGB(40, 25, 50)
            SliderBack.BorderSizePixel = 0
            SliderBack.Parent = SliderFrame
            Instance.new("UICorner", SliderBack).CornerRadius = UDim.new(0, 3)
            
            local SliderFill = Instance.new("Frame")
            SliderFill.Size = UDim2.new((value - min) / (max - min), 0, 1, 0)
            SliderFill.BackgroundColor3 = color
            SliderFill.BorderSizePixel = 0
            SliderFill.Parent = SliderBack
            Instance.new("UICorner", SliderFill).CornerRadius = UDim.new(0, 3)
            
            local dragging = false
            
            local function update(input)
                local pos = math.clamp((input.Position.X - SliderBack.AbsolutePosition.X) / SliderBack.AbsoluteSize.X, 0, 1)
                value = math.floor(min + (max - min) * pos)
                Tween(SliderFill, {Size = UDim2.new(pos, 0, 1, 0)}, 0.08)
                ValueLabel.Text = tostring(value)
                callback(value)
            end
            
            SliderBack.InputBegan:Connect(function(input)
                if input.UserInputType == Enum.UserInputType.MouseButton1 or
                   input.UserInputType == Enum.UserInputType.Touch then
                    dragging = true
                    update(input)
                end
            end)
            
            SliderBack.InputEnded:Connect(function(input)
                if input.UserInputType == Enum.UserInputType.MouseButton1 or
                   input.UserInputType == Enum.UserInputType.Touch then
                    dragging = false
                end
            end)
            
            UserInputService.InputChanged:Connect(function(input)
                if dragging and (input.UserInputType == Enum.UserInputType.MouseMovement or
                   input.UserInputType == Enum.UserInputType.Touch) then
                    update(input)
                end
            end)
        end
        
        function Tab:Textbox(text, placeholder, callback)
            callback = callback or function() end
            
            local TextFrame = Instance.new("Frame")
            TextFrame.Size = UDim2.new(1, -6, 0, 62)
            TextFrame.BackgroundColor3 = Color3.fromRGB(30, 18, 40)
            TextFrame.BorderSizePixel = 0
            TextFrame.Parent = TabContent
            Instance.new("UICorner", TextFrame).CornerRadius = UDim.new(0, 6)
            
            local TextLabel = Instance.new("TextLabel")
            TextLabel.Size = UDim2.new(1, -16, 0, 18)
            TextLabel.Position = UDim2.fromOffset(10, 6)
            TextLabel.BackgroundTransparency = 1
            TextLabel.Text = text
            TextLabel.Font = Enum.Font.Gotham
            TextLabel.TextSize = 14
            TextLabel.TextColor3 = Color3.fromRGB(200, 150, 255)
            TextLabel.TextXAlignment = Enum.TextXAlignment.Left
            TextLabel.Parent = TextFrame
            
            local Textbox = Instance.new("TextBox")
            Textbox.Size = UDim2.new(1, -20, 0, 30)
            Textbox.Position = UDim2.fromOffset(10, 28)
            Textbox.BackgroundColor3 = Color3.fromRGB(40, 25, 50)
            Textbox.BorderSizePixel = 0
            Textbox.Text = ""
            Textbox.PlaceholderText = placeholder or "Enter text..."
            Textbox.Font = Enum.Font.Gotham
            Textbox.TextSize = 13
            Textbox.TextColor3 = Color3.white
            Textbox.PlaceholderColor3 = Color3.fromRGB(120, 80, 150)
            Textbox.ClearTextOnFocus = false
            Textbox.Parent = TextFrame
            Instance.new("UICorner", Textbox).CornerRadius = UDim.new(0, 6)
            
            local TextStroke = Instance.new("UIStroke")
            TextStroke.Color = Color3.fromRGB(80, 40, 120)
            TextStroke.Thickness = 1
            TextStroke.Parent = Textbox
            
            local TextPadding = Instance.new("UIPadding")
            TextPadding.PaddingLeft = UDim.new(0, 8)
            TextPadding.PaddingRight = UDim.new(0, 8)
            TextPadding.Parent = Textbox
            
            Textbox.Focused:Connect(function()
                Tween(Textbox, {BackgroundColor3 = Color3.fromRGB(50, 30, 60)}, 0.2)
                Tween(TextStroke, {Color = color}, 0.2)
            end)
            
            Textbox.FocusLost:Connect(function(enter)
                Tween(Textbox, {BackgroundColor3 = Color3.fromRGB(40, 25, 50)}, 0.2)
                Tween(TextStroke, {Color = Color3.fromRGB(80, 40, 120)}, 0.2)
                if enter then
                    callback(Textbox.Text)
                end
            end)
        end
        
        function Tab:Dropdown(text, options, default, callback)
            callback = callback or function() end
            options = options or {}
            local selected = default or (options[1] or "Select")
            
            local DropFrame = Instance.new("Frame")
            DropFrame.Size = UDim2.new(1, -6, 0, 62)
            DropFrame.BackgroundColor3 = Color3.fromRGB(30, 18, 40)
            DropFrame.BorderSizePixel = 0
            DropFrame.Parent = TabContent
            Instance.new("UICorner", DropFrame).CornerRadius = UDim.new(0, 6)
            
            local DropLabel = Instance.new("TextLabel")
            DropLabel.Size = UDim2.new(1, -16, 0, 18)
            DropLabel.Position = UDim2.fromOffset(10, 6)
            DropLabel.BackgroundTransparency = 1
            DropLabel.Text = text
            DropLabel.Font = Enum.Font.Gotham
            DropLabel.TextSize = 14
            DropLabel.TextColor3 = Color3.fromRGB(200, 150, 255)
            DropLabel.TextXAlignment = Enum.TextXAlignment.Left
            DropLabel.Parent = DropFrame
            
            local DropBtn = Instance.new("TextButton")
            DropBtn.Size = UDim2.new(1, -20, 0, 30)
            DropBtn.Position = UDim2.fromOffset(10, 28)
            DropBtn.BackgroundColor3 = Color3.fromRGB(40, 25, 50)
            DropBtn.BorderSizePixel = 0
            DropBtn.Text = selected
            DropBtn.Font = Enum.Font.Gotham
            DropBtn.TextSize = 13
            DropBtn.TextColor3 = Color3.white
            DropBtn.AutoButtonColor = false
            DropBtn.Parent = DropFrame
            Instance.new("UICorner", DropBtn).CornerRadius = UDim.new(0, 6)
            
            local DropStroke = Instance.new("UIStroke")
            DropStroke.Color = Color3.fromRGB(80, 40, 120)
            DropStroke.Thickness = 1
            DropStroke.Parent = DropBtn
            
            local Arrow = Instance.new("TextLabel")
            Arrow.Size = UDim2.fromOffset(20, 30)
            Arrow.Position = UDim2.new(1, -24, 0, 0)
            Arrow.BackgroundTransparency = 1
            Arrow.Text = "▼"
            Arrow.Font = Enum.Font.GothamBold
            Arrow.TextSize = 10
            Arrow.TextColor3 = Color3.fromRGB(150, 100, 200)
            Arrow.Parent = DropBtn
            
            local DropList = Instance.new("Frame")
            DropList.Size = UDim2.new(1, -20, 0, math.min(#options * 28, 140))
            DropList.Position = UDim2.fromOffset(10, 62)
            DropList.BackgroundColor3 = Color3.fromRGB(35, 20, 45)
            DropList.BorderSizePixel = 0
            DropList.Visible = false
            DropList.ZIndex = 10
            DropList.Parent = DropFrame
            Instance.new("UICorner", DropList).CornerRadius = UDim.new(0, 6)
            
            local ListStroke = Instance.new("UIStroke")
            ListStroke.Color = color
            ListStroke.Thickness = 2
            ListStroke.Parent = DropList
            
            local ListScroll = Instance.new("ScrollingFrame")
            ListScroll.Size = UDim2.new(1, -6, 1, -6)
            ListScroll.Position = UDim2.fromOffset(3, 3)
            ListScroll.BackgroundTransparency = 1
            ListScroll.BorderSizePixel = 0
            ListScroll.ScrollBarThickness = 3
            ListScroll.ScrollBarImageColor3 = color
            ListScroll.CanvasSize = UDim2.fromOffset(0, #options * 28)
            ListScroll.Parent = DropList
            
            local ListLayout = Instance.new("UIListLayout")
            ListLayout.Padding = UDim.new(0, 2)
            ListLayout.Parent = ListScroll
            
            local expanded = false
            
            for _, option in ipairs(options) do
                local OptBtn = Instance.new("TextButton")
                OptBtn.Size = UDim2.new(1, -4, 0, 26)
                OptBtn.BackgroundColor3 = Color3.fromRGB(40, 25, 50)
                OptBtn.BorderSizePixel = 0
                OptBtn.Text = tostring(option)
                OptBtn.Font = Enum.Font.Gotham
                OptBtn.TextSize = 12
                OptBtn.TextColor3 = Color3.fromRGB(200, 150, 255)
                OptBtn.AutoButtonColor = false
                OptBtn.Parent = ListScroll
                Instance.new("UICorner", OptBtn).CornerRadius = UDim.new(0, 5)
                
                OptBtn.MouseButton1Click:Connect(function()
                    selected = tostring(option)
                    DropBtn.Text = selected
                    expanded = false
                    Tween(DropFrame, {Size = UDim2.new(1, -6, 0, 62)}, 0.2)
                    task.wait(0.2)
                    DropList.Visible = false
                    Arrow.Text = "▼"
                    callback(option)
                end)
                
                OptBtn.MouseEnter:Connect(function()
                    Tween(OptBtn, {BackgroundColor3 = Color3.fromRGB(80, 40, 120)}, 0.15)
                end)
                
                OptBtn.MouseLeave:Connect(function()
                    Tween(OptBtn, {BackgroundColor3 = Color3.fromRGB(40, 25, 50)}, 0.15)
                end)
            end
            
            DropBtn.MouseButton1Click:Connect(function()
                expanded = not expanded
                if expanded then
                    local newHeight = 62 + math.min(#options * 28, 140) + 6
                    Tween(DropFrame, {Size = UDim2.new(1, -6, 0, newHeight)}, 0.2)
                    DropList.Visible = true
                    Arrow.Text = "▲"
                else
                    Tween(DropFrame, {Size = UDim2.new(1, -6, 0, 62)}, 0.2)
                    task.wait(0.2)
                    DropList.Visible = false
                    Arrow.Text = "▼"
                end
            end)
            
            DropBtn.MouseEnter:Connect(function()
                Tween(DropBtn, {BackgroundColor3 = Color3.fromRGB(50, 30, 60)}, 0.2)
            end)
            
            DropBtn.MouseLeave:Connect(function()
                Tween(DropBtn, {BackgroundColor3 = Color3.fromRGB(40, 25, 50)}, 0.2)
            end)
        end
        
        function Tab:Line()
            local Line = Instance.new("Frame")
            Line.Size = UDim2.new(1, -6, 0, 2)
            Line.BackgroundColor3 = Color3.fromRGB(80, 40, 120)
            Line.BorderSizePixel = 0
            Line.Parent = TabContent
        end
        
        function Tab:Spacing(height)
            local Space = Instance.new("Frame")
            Space.Size = UDim2.new(1, -6, 0, height or 8)
            Space.BackgroundTransparency = 1
            Space.Parent = TabContent
        end
        
        return Tab
    end
    
    function Window:Notify(title, content, duration)
        spawn(function()
            duration = duration or 3
            
            local Notif = Instance.new("Frame")
            Notif.Size = UDim2.new(1, 0, 0, 70)
            Notif.BackgroundColor3 = Color3.fromRGB(25, 15, 35)
            Notif.BorderSizePixel = 0
            Notif.Parent = NotifHolder
            Instance.new("UICorner", Notif).CornerRadius = UDim.new(0, 8)
            
            local NotifStroke = Instance.new("UIStroke")
            NotifStroke.Color = color
            NotifStroke.Thickness = 2
            NotifStroke.Parent = Notif
            
            local NotifTitle = Instance.new("TextLabel")
            NotifTitle.Size = UDim2.new(1, -16, 0, 20)
            NotifTitle.Position = UDim2.fromOffset(8, 6)
            NotifTitle.BackgroundTransparency = 1
            NotifTitle.Text = title or "Notification"
            NotifTitle.Font = Enum.Font.GothamBold
            NotifTitle.TextSize = 15
            NotifTitle.TextColor3 = Color3.fromRGB(200, 150, 255)
            NotifTitle.TextXAlignment = Enum.TextXAlignment.Left
            NotifTitle.Parent = Notif
            
            local NotifContent = Instance.new("TextLabel")
            NotifContent.Size = UDim2.new(1, -16, 0, 36)
            NotifContent.Position = UDim2.fromOffset(8, 28)
            NotifContent.BackgroundTransparency = 1
            NotifContent.Text = content or "No content"
            NotifContent.Font = Enum.Font.Gotham
            NotifContent.TextSize = 13
            NotifContent.TextColor3 = Color3.fromRGB(180, 140, 220)
            NotifContent.TextXAlignment = Enum.TextXAlignment.Left
            NotifContent.TextYAlignment = Enum.TextYAlignment.Top
            NotifContent.TextWrapped = true
            NotifContent.Parent = Notif
            
            local Progress = Instance.new("Frame")
            Progress.Size = UDim2.new(1, 0, 0, 3)
            Progress.Position = UDim2.new(0, 0, 1, -3)
            Progress.BackgroundColor3 = color
            Progress.BorderSizePixel = 0
            Progress.Parent = Notif
            Instance.new("UICorner", Progress).CornerRadius = UDim.new(0, 2)
            
            Tween(Progress, {Size = UDim2.new(0, 0, 0, 3)}, duration)
            
            task.wait(duration)
            
            Tween(Notif, {Size = UDim2.new(1, 0, 0, 0)}, 0.25)
            task.wait(0.25)
            Notif:Destroy()
        end)
    end
    
    function Window:Destroy()
        ScreenGui:Destroy()
    end
    
    -- Entrance animation
    Main.Size = UDim2.fromOffset(0, 0)
    Tween(Main, {Size = UDim2.fromOffset(380, 420)}, 0.4)
    
    -- Welcome notification
    if config.WelcomeMessage then
        task.wait(0.5)
        Window:Notify("Welcome!", config.WelcomeMessage, 4)
    end
    
    return Window
end

return VoidMobile